  <%- include('header') -%>
    <div class="box" id="heading">
<h1><%=title %> </h1>
    </div>
  <div class="box">

      <%  for (var i=0; i<=item.length; i++){ %>
        <div class="item">
          <input type="checkbox">
            <p class="item"><%=item[i]%></p>
            </div>
        <%  }%>



      <form class="item" action="/" method="post" >
        <input type="text" name="newItem" value="" placeholder="New item" autocomplete="off">
        <button type="submit" name="btn" value= <%= title %> >+</button>

      </form>

  </div>
  <%- include('footer') -%>
